xecution\r\n            }\r\n\r\n            setUser(data);\r\n\r\n        } catch(err){\r\n            setError('Something went wrong' + err)\r\n        }\r\n\r\n\r\n    }\r\n\r\n    return (\r\n        <div>\r\n            <h2>\r\n                Login\r\n            </h2>\r\n\r\n            {/* form for login */}\r\n            <form onSubmit={handleSubmit}>\r\n\r\n                {/* email field */}\r\n                <input\r\n                    type = 'email'\r\n                    value = {email}\r\n                    onChange = {(event) => {\r\n                        setError('');\r\n                        setEmail(event.target.value)\r\n                    }}\r\n                />\r\n                <br/>\r\n\r\n                {/* password field */}\r\n                <input\r\n                    type = 'password'\r\n                    value = {password}\r\n                    onChange = {(event) => {\r\n                        setError('');\r\n                        setPassword(event.target.value)}\r\n                    }\r\n                />\r\n                <br/>\r\n\r\n                {/* submit button */}\r\n                <button>Login</button>\r\n            </form>\r\n\r\n            {/* link to sign up page */}\r\n            <p>Don't have an account? <Link to=\"/signup\">Sign up!</Link></p>\r\n\r\n            {error && <p>{error}</p>}\r\n\r\n        </div>\r\n    )\r\n}\r\n","C:\\Users\\Joe\\Desktop\\CSI-TTP\\Capstone\\Frontend\\ttp-capstone-art-store-client\\src\\pages\\Home.js",[],"C:\\Users\\Joe\\Desktop\\CSI-TTP\\Capstone\\Frontend\\ttp-capstone-art-store-client\\src\\pages\\SingleArtist.js",["134","135"],"\r\nimport React, { useEffect, useState } from \"react\";\r\nimport { useParams } from \"react-router-dom\";\r\n//import { Link } from \"react-router-dom\";\r\n\r\nexport default function SingleArtist() {\r\n  const userId = useParams().id;\r\n\r\n  const [User, setUser] = useState([]);\r\n\r\n  //fetching one product's data fro