{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "name": "product",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "attributes": {
    "title": {
      "type": "string"
    },
    "date": {
      "type": "date"
    },
    "image": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images",
        "files",
        "videos"
      ],
      "plugin": "upload",
      "required": false
    },
    "description": {
      "type": "richtext"
    },
    "artist": {
      "via": "products",
      "plugin": "users-permissions",
      "model": "user"
    },
    "price": {
      "type": "decimal"
    },
    "category": {
      "type": "string"
    }
  }
}
